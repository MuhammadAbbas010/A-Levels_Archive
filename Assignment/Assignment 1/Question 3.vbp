Imports System

Module Program
    Sub Main(args As String())
        Dim PrevNum As Integer
        Dim num As Integer = 0
        Dim display As Integer = 1

        For num = 1 To 20
            Console.Write("{0}", display)
            PrevNum = num
            num = num + 1
            display = PrevNum + num
        Next
        Console.Write("...")
        Console.ReadLine()
    End Sub
End Module
