Module Module3
    Sub Main()
        Dim Marks(4) As Integer
        Dim Row As Integer

        For Row = 0 To 4
            Console.WriteLine("Enter work for student {0} term 1", Row + 1)
            Marks(Row) = CInt(Console.ReadLine())
        Next

        For Row = 0 To 4
            Console.WriteLine(Marks(Row))
        Next

        Console.ReadLine()
    End Sub

End Module
